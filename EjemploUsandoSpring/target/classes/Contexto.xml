<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.1.xsd
    "
    >
	<bean id="Biblioteca" class="ModeloImp.Biblioteca"></bean>
	<bean id="Persona" class="ModeloImp.Persona"  scope="prototype"></bean>
	<bean id= "ControladorBusiness" class="ControladoresImp.ControladorBusinessImp">
	<property name="biblioteca" ref="Biblioteca"></property>
	</bean>
	<bean id= "ControladorInput" class="ControladoresImp.ControladorInputImp"></bean>

</beans>